paises = ['Afeganistão', 'África do Sul', 'Albânia', 'Alemanha', 'Andorra', 'Angola', 'Antígua e Barbuda', 'Arábia Saudita', 'Argélia', 'Argentina', 'Armênia', 'Austrália', 'Áustria', 'Azerbaijão']

def posicao_pais(nome):
    return paises.index(nome)

# exemplo de uso
print(posicao_pais('Brasil'))

numeros = []

def adicionar_inicio(num):
    numeros.insert(0, num)

def adicionar_fim(num):
    numeros.append(num)

# exemplo de uso
adicionar_inicio(10)
adicionar_fim(20)
print(numeros)
